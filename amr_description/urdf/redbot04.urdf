<robot name="redbot04">
  <material name="red">
    <color rgba="1.0 0.1 0.1 1" />
  </material>
  <material name="gray">
    <color rgba="0.3 0.3 0.3 1" />
  </material>
  <material name="black">
    <color rgba="0.1 0.1 0.1 1" />
  </material>

  <link name="base_footprint" />

  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" />
      <geometry>
        <box size="0.710 0.505 0.320" />
      </geometry>
      <material name="red" />
    </visual>
  </link>

  <joint name="base_link_link_joint" type="fixed">
    <parent link="base_footprint" />
    <child link="base_link" />
    <origin xyz="0 0 0.114" />
  </joint>

  <link name="lidar_scan">
    <visual>
      <origin xyz="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.03" />
      </geometry>
      <material name="gray" />
    </visual>
  </link>
  <!-- 추후 Lidar 위치에 따라 x,z 값 조정 필요-->

  <joint name="laser_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="lidar_scan" />
    <origin xyz="0.33 0 0.221" /> 
  </joint>

  <link name="imu_link"/>
  <!-- 추후 IMU 위치에 따라 x,z 값 조정 필요-->

  <joint name="imu_link_joint" type="fixed">
    <parent link="base_link" />
    <child link="imu_link" />
    <origin xyz="0.0 0.0 0.15" /> 
  </joint>

  <joint name="camera_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.325 0 0.271"/>
    <parent link="base_link"/>
    <child link="camera_bottom_screw_frame"/>
  </joint>
  <link name="camera_bottom_screw_frame"/>
  <joint name="camera_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.011150000000000002 0.0475 0.0145"/>
    <parent link="camera_bottom_screw_frame"/>
    <child link="camera_link"/>
  </joint>

  <link name="camera_link">
    <visual>
      <!-- the mesh origin is at front plate in between the two infrared camera axes -->
      <!-- <origin xyz="${d455_zero_depth_to_glass-d455_cam_depth/2} ${-d455_cam_depth_py} 0" rpy="0 0 0"/> -->
      <origin rpy="1.5707963267948966 0 1.5707963267948966" xyz="0.0046500000000000005 -0.0475 0"/>
      <geometry>
        <!-- <box size="${d455_cam_depth} ${d455_cam_width} ${d455_cam_height}"/> -->
        <mesh filename="package://redbot02_description/meshes/d455.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="aluminum"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="-0.00845 -0.0475 0"/>
      <geometry>
        <box size="0.026 0.124 0.029"/>
      </geometry>
    </collision>
    <inertial>
      <!-- The following are not reliable values, and should not be used for modeling -->
      <mass value="0.072"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.003881243" ixy="0.0" ixz="0.0" iyy="0.000498940" iyz="0.0" izz="0.003879257"/>
    </inertial>
  </link>

